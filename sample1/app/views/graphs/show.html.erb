<div class="page-header">
  <%= link_to graphs_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_graph_path(@graph), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show graph</h1>
</div>

<dl class="dl-horizontal">
  <dt>X:</dt>
  <dd><%= @graph.x %></dd>

  <dt>Y:</dt>
  <dd><%= @graph.y %></dd>

</dl>
