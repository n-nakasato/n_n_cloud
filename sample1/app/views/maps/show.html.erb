<div class="page-header">
  <%= link_to maps_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_map_path(@map), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show map</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @map.title %></dd>

  <dt>Description:</dt>
  <dd><%= @map.description %></dd>

  <dt>Address:</dt>
  <dd><%= @map.address %></dd>

  <dt>Latitude:</dt>
  <dd><%= @map.latitude %></dd>

  <dt>Longitude:</dt>
  <dd><%= @map.longitude %></dd>

</dl>
